<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">
	<h:head>
		<h:title>HCT Household Survey</h:title>
		<model>
			<instance>
				<data name="HCT Household Survey" version="1.0">
					<meta>
						<version/>
						<start_time/>
						<end_time/>
						<device_id/>
						<subscriber_id/>
					</meta>
					<survey>
						<survey_location/>
						<provider_id/>
						<team_id/>
						<survey_id/>
					</survey>
					<household>
						<household_id/>
						<village/>
						<location/>
						<sublocation/>
						<district/>
						<division/>
						<household_adults/>
						<children_under13/>
						<total_household/>
						<allowed_in/>
						<conduct_survey_group/>
						<economic_questions_group/>
						<bednet_questions_group/>
						<return_date/>
						<number_of_bednets/>
						<bednets_given/>
						<bednet_voucher/>
					</household>
					<economics>
						<land_owned/>
						<cows_owned/>
						<goats_owned/>
						<sheep_owned/>
						<children_in_household/>
						<children_in_school/>
					</economics>
					<individual>
						<household/>
						<hct_id_barcode/>
						<hct_id_manual/>
						<survey_individual_id/>
						<given_name/>
						<middle_name/>
						<family_name/>
						<gender/>
						<age_type/>
						<birth_date/>
						<age_in_years/>
						<age_in_months/>
						<approximate_age/>
						<phone_number/>
						<reenter_phone_number/>
						<demographics/>
						<marital_status/>
						<house_status/>
						<relation_to_househead/>
<!-- Indiv Economics -->
						<hours_in_farm/>
						<hours_in_housework/>
						<clinic_visits/>
						<cost_of_medication/>
<!-- End of Indiv Economics -->
						<pregnancy_status/>
						<attending_anc/>
						<children_below18_group/>
						<children_below5_group/>
						<mother_died/>
						<mother_hiv_positive/>
						<father_died/>
						<father_hiv_positive/>
						<child_needs_hiv_test/>
						<test_child_regardless/>
						<immunization_info/>
						<child_immunized/>
						<immunization_info_source/>
						<bcg_immunization/>
						<measles_immunization/>
						<previous_hiv_test/>
						<previous_hiv_test_year/>
						<previous_hiv_test_results/>
						<hiv_tx_program_status/>
						<hiv_tx_program_name/>
						<ampath_id/>
						<hiv_testing_group/>
						<pretest_counseling/>
						<accepted_hiv_testing/>
						<reasons_for_decline/>
						<hiv_results_group/>
						<test1_result/>
						<test2_name/>
						<test2_result/>
						<test3_name/>
						<test3_result/>
						<test_outcome/>
						<couple_tested_together/>
						<discordant_couple/>
						<partner_hct_id/>
						<tb_screening_group/>
						<current_tb_tx_status/>
						<tb_info_group/>
						<past_tb_tx_status/>
						<past_tb_tx_year/>
						<completed_8_mths_tb_tx/>
						<cough_gt_2_wks/>
						<blood_cough_past_year/>
						<fever_gt_3_wks/>
						<weight_loss_past_year/>
						<tb_in_household/>
						<failure_to_thrive/>
						<sputum_collected/>
						<follow_up/>
						<follow_up_date/>
					</individual>
				</data>
			</instance>
			<!-- Meta Information Bindings -->
			<bind nodeset="/data/meta/start_time" jr:preload="timestamp" jr:preloadParams="start" type="time"/>
			<bind nodeset="/data/meta/end_time" jr:preload="timestamp" jr:preloadParams="end" type="time"/>
			<bind nodeset="/data/meta/device_id" jr:preload="property" jr:preloadParams="deviceid" type="string"/>
			<bind nodeset="/data/meta/subscriber_id" jr:preload="property" jr:preloadParams="subscriberid" type="string"/>
			<!-- Survey Information Bindings -->
			<bind nodeset="/data/survey/survey_location" required="true()" type="geopoint"/>
			<bind nodeset="/data/survey/survey_id" type="integer"/>

			<bind nodeset="/data/survey/provider_id"  constraint="regex(., '^\w+[-]?\d{1}') and (checkdigit(.))" required="true()" 
					jr:constraintMsg="Invalid Provider ID. Please check and re-enter." type="string"/>
			<bind nodeset="/data/survey/team_id" required="true()" type="string"/>
			<!-- Household Information Bindings -->
			<bind nodeset="/data/household/household_id" constraint="regex(., '^\w+[-]?\d{1}') and checkdigit(.) and unique('household_id', .)" 
					required="true()" jr:constraintMsg="Invalid Household ID. Please check and re-enter." type="string"/>
			<bind nodeset="/data/household/village" required="true()" type="string"/>
			<bind nodeset="/data/household/location" required="true()" type="string"/>
			<bind nodeset="/data/household/sublocation" required="true()" type="string"/>
			<bind nodeset="/data/household/district" required="true()" type="string"/>
			<bind nodeset="/data/household/division" required="true()" type="string"/>
			<bind nodeset="/data/household/household_adults" required="true()" constraint=". &gt;= 0" 
					jr:constraintMsg="Number of adults must be positive. If none, enter 0." type="integer"/>
			<bind nodeset="/data/household/children_under13" required="true()" constraint=". &gt;= 0" 
					jr:constraintMsg="Number of children must be positive. If none, enter 0." type="integer"/>
			<bind nodeset="/data/household/total_household" calculate="../household_adults + ../children_under13" constraint=". &gt;= 1" jr:constraintMsg="Total in household must be greater than one." readonly="true()" type="integer"/>
			<bind nodeset="/data/household/allowed_in" required="true()" type="string"/>
			<bind nodeset="/data/household/return_date" relevant="../allowed_in='nothome' or ../allowed_in='laterdate'" 
					constraint=". &gt;= today()" jr:constraintMsg="Return date must be after today." type="date"/>
			<bind nodeset="/data/household/number_of_bednets" required="true()" constraint=". &gt;= 0" 
					jr:constraintMsg="Number of bed nets must be positive. If none, enter 0." type="integer"/>
			<bind nodeset="/data/household/bednets_given" required="true()" type="string"/>
			<bind nodeset="/data/household/bednet_voucher" relevant="../bednets_given != 0" type="string"/>
			<bind nodeset="/data/economics/land_owned" constraint=". &gt;= 0" required="true()" jr:constraintMsg="Land owned must be positive. If none, enter 0." type="decimal"/>
			<bind nodeset="/data/economics/cows_owned" constraint=". &gt;= 0" required="true()" jr:constraintMsg="Cows owned must be positive. If none, enter 0." type="integer"/>
			<bind nodeset="/data/economics/goats_owned" constraint=". &gt;= 0" required="true()" jr:constraintMsg="Goats owned must be positive. If none, enter 0." type="integer"/>
			<bind nodeset="/data/economics/sheep_owned" constraint=". &gt;= 0" required="true()" jr:constraintMsg="Sheep owned must be positive. If none, enter 0." type="integer"/>
			<bind nodeset="/data/economics/children_in_household" constraint=". &gt;= 0" jr:constraintMsg="Number of children must be positive. If none, enter 0." required="true()" type="integer"/>
			<!-- constraint message not working -->
			<bind nodeset="/data/economics/children_in_school" relevant="../children_in_household &gt; 0" constraint=". &lt;= ../children_in_household  and . &gt;= 0" jr:constraintMsg="Number of children in school must be less than the number in household and positive." type="integer"/>
			<!-- Individual Data Bindings -->
			<bind nodeset="/data/individual/household" constraint="review('Household ID', .)" 
					calculate="/data/household/household_id" readonly="true()" type="string" />
			<bind nodeset="/data/individual/hct_id_barcode" 
					constraint="regex(., '^\w+[-]?\d{1}') and checkdigit(.) and unique('hct_id', .) and review('HCT ID', .)" 
					jr:constraintMsg="Invalid HCT ID. Please check and re-enter." type="barcode"/>
			<bind nodeset="/data/individual/hct_id_manual" 
					constraint="regex(., '^\w+[-]?\d{1}') and checkdigit(.) and unique('hct_id', .) and review('HCT ID', .)" 
					relevant="../hct_id_barcode = '' " required="true()" jr:constraintMsg="Invalid HCT ID. Please check and re-enter." type="string"/>
			<bind nodeset="/data/individual/given_name" constraint="review('Given Name', .)" required="true()" type="string"/>
			<bind nodeset="/data/individual/middle_name" constraint="review('Middle Name', .)" type="string"/>
			<bind nodeset="/data/individual/family_name" constraint="review('Family Name', .)" required="true()" type="string"/>
			<bind nodeset="/data/individual/gender" constraint="review('Gender', .)" required="true()" type="string"/>
			<bind nodeset="/data/individual/age_type" required="true()" type="string"/>
			<bind nodeset="/data/individual/birth_date" relevant="../age_type='birth_date'" constraint=". &lt;= today()" 
					jr:constraintMsg="Birth dates must be before today." type="date"/>
			<bind nodeset="/data/individual/age_in_years" constraint=". &gt;= 0 and . &lt;= 110" 
					relevant="../age_type='age_in_years_months' or ../age_type='unknown'" jr:constraintMsg="Age must be between 0 and 110." required="true()" type="integer"/>
			<bind nodeset="/data/individual/age_in_months" constraint=". &gt;= 0 and . &lt;= 11" 
					relevant="(../age_type='age_in_years_months' or ../age_type='unknown') and ../age_in_years &lt; 5" jr:constraintMsg="Age must be between 0 and 11." required="true()" type="integer"/>
			<bind nodeset="/data/individual/approximate_age" readonly="true()" type="integer" 
					calculate="if(../age_type='birth_date', ((now()-../birth_date) div 30), 
					(if(../age_in_months &gt;0, ((../age_in_years * 12)+../age_in_months), (../age_in_years * 12))))"/>
			<bind nodeset="/data/individual/phone_number" required="true()" 
					constraint="(. = '0' or regex(., '\d{8,12}')) and review('Phone Number', .) and review('Year of Birth', ../approximate_age)"   						jr:constraintMsg="Invalid Phone Number. Please check and re-enter." type="string"/>
			<bind nodeset="/data/individual/reenter_phone_number" type="string" relevant="../phone_number != ''" 
					required="true()" constraint=". = ../phone_number" jr:constraintMsg="Phone numbers do not match. Go back and confirm both."/>
			<!-- can't have review type -->
			<bind nodeset="/data/individual/demographics" type="review"/>
			<bind nodeset="/data/individual/marital_status" required="true()" relevant="../approximate_age &gt;= 156" type="string"/>
			<bind nodeset="/data/individual/house_status" type="string"/>
			<bind nodeset="/data/individual/relation_to_househead" required="true()" type="string"/>
			<!-- Individual Economic Questions -->
			<bind nodeset="/data/individual/hours_in_farm" constraint=". &gt;= 0" relevant="../approximate_age &gt;= 156" jr:constraintMsg="Hours worked on farm owned must be positive. If none, enter 0." relevant="../approximate_age &gt;= 96" type="integer"/>
			<bind nodeset="/data/individual/hours_in_housework" constraint=". &gt;= 0" relevant="../approximate_age &gt;= 156" jr:constraintMsg="Hours spent on housework must be positive. If none, enter 0." relevant="../approximate_age &gt;= 96" type="integer"/>
			<bind nodeset="/data/individual/clinic_visits" constraint=". &gt;= 0" jr:constraintMsg="Number of clinic visits must be positive. If none, enter 0." type="integer"/>
			<bind nodeset="/data/individual/cost_of_medication" constraint=". &gt;= 0" jr:constraintMsg="Amount spent on medication must be positive. If none, enter 0." type="integer"/>
			<!-- End of Individual Economic Questions -->
			<bind nodeset="/data/individual/pregnancy_status" required="true()" relevant="../gender='f' and ../approximate_age &gt;= 96" type="string"/>
			<bind nodeset="/data/individual/attending_anc" required="true()" relevant="../pregnancy_status='y'" type="string"/>
			<bind nodeset="/data/individual/mother_died" required="true()" type="string"/>
			<bind nodeset="/data/individual/mother_hiv_positive" required="true()" type="string"/>
			<bind nodeset="/data/individual/father_died" required="true()" type="string"/>
			<bind nodeset="/data/individual/father_hiv_positive" required="true()" type="string"/>
			<bind nodeset="/data/individual/child_needs_hiv_test" readonly="true()" 
					calculate="if(../approximate_age &lt; 156 and (../mother_died='y' or ../mother_died='unknown' or ../mother_hiv_positive='y' or 
					../mother_hiv_positive='unknown'), 'Needs Testing', 'Does Not Need Testing')" 	
					relevant="../approximate_age &lt; 156 and (../mother_died='y' or ../mother_died='unknown' or ../mother_hiv_positive='y' or 
					../mother_hiv_positive='unknown')" type="string"/>
			<bind nodeset="/data/individual/test_child_regardless" type="string"
					relevant="../approximate_age &lt; 156 and (../mother_died='n' and ../mother_hiv_positive='n')" />
			<bind nodeset="/data/individual/child_immunized" type="string"/>
			<bind nodeset="/data/individual/immunization_info_source" type="string"/>
			<bind nodeset="/data/individual/bcg_immunization" required="true()" type="string"/>
			<bind nodeset="/data/individual/measles_immunization" required="true()" type="string"/>
			<bind nodeset="/data/individual/previous_hiv_test" required="true()" type="string"/>
			<bind nodeset="/data/individual/previous_hiv_test_year" required="true()" relevant="../previous_hiv_test='y'" type="string"/>
			<bind nodeset="/data/individual/previous_hiv_test_results" required="true()" relevant="../previous_hiv_test='y'" type="string"/>
			<bind nodeset="/data/individual/hiv_tx_program_status" relevant="../previous_hiv_test_results='positive'" type="string"/>
			<bind nodeset="/data/individual/hiv_tx_program_name" relevant="../hiv_tx_program_status='y'" type="string"/>
			<bind nodeset="/data/individual/ampath_id" type="string" 
					relevant="../hiv_tx_program_name='Ampath' or ../hiv_tx_program_name='AMPATH' or ../hiv_tx_program_name='ampath'"/>
			<bind nodeset="/data/individual/pretest_counseling" required="true()" type="string"/>
			<bind nodeset="/data/individual/accepted_hiv_testing" required="true()" type="string" 
					constraint=". = 'n' or (. ='y' and ../pretest_counseling='y')" jr:constraintMsg="You can not test without counseling"/>
			<bind nodeset="/data/individual/reasons_for_decline" required="true()" type="string" 
					relevant="../accepted_hiv_testing='n' and ../approximate_age &lt; 156" constraint="count-selected(.) &lt;= 3" 
					jr:constraintMsg="Only a maximum of three allowed."/>
			<bind nodeset="/data/individual/test1_result" required="true()" type="string"/>
			<bind nodeset="/data/individual/test2_name" required="true()" type="string"/>
			<bind nodeset="/data/individual/test2_result" required="true()" type="string"/>
			<bind nodeset="/data/individual/test3_name" required="true()" relevant="../test2_result != ../test1_result" 
					constraint=". != ../test2_name" jr:constraintMsg="Test 3 must be different from Test 2!" type="string"/>
			<bind nodeset="/data/individual/test3_result" required="true()" relevant="../test2_result != ../test1_result" type="string"/>
			<bind nodeset="/data/individual/test_outcome" readonly="true()" relevant="../test2_result != '' or ../test3_result != ''" type="string" 
					calculate="if (../test3_result = 'reactive' or ((../test1_result = ../test2_result) and ../test2_result = 'reactive'),'Please refer patient to AMPATH', if (../test3_result = 'nonreactive' or ((../test1_result = ../test2_result) and ../test2_result = 'nonreactive'),'Patient is Negative', 'Refer patient to a clinic for further testing.'))"/>
			<bind nodeset="/data/individual/couple_tested_together" required="true()" relevant="../marital_status='married'" type="string"/>
			<bind nodeset="/data/individual/discordant_couple" required="true()" relevant="../marital_status='married'" type="string"/>
			<!-- check partner hct id -->
			<bind nodeset="/data/individual/partner_hct_id" constraint="regex(., '^\w+[-]?\d{1}') and checkdigit(.)" required="true()" 
					relevant="../discordant_couple='y'" type="string"/>
			<bind nodeset="/data/individual/current_tb_tx_status" required="true()" type="string"/>
			<bind nodeset="/data/individual/past_tb_tx_status" required="true()" type="string"/>
			<bind nodeset="/data/individual/past_tb_tx_year" required="true()" relevant="../past_tb_tx_status='y'" type="string"/>
			<bind nodeset="/data/individual/completed_8_mths_tb_tx" required="true()" relevant="../past_tb_tx_status='y'" type="string"/>
			<bind nodeset="/data/individual/cough_gt_2_wks" required="true()" type="string"/>
			<bind nodeset="/data/individual/blood_cough_past_year" required="true()" type="string"/>
			<bind nodeset="/data/individual/fever_gt_3_wks" required="true()" type="string"/>
			<bind nodeset="/data/individual/weight_loss_past_year" required="true()" type="string"/>
			<bind nodeset="/data/individual/tb_in_household" required="true()" type="string"/>
			<bind nodeset="/data/individual/failure_to_thrive" relevant="../approximate_age &lt; 156" type="string"/>
			<bind nodeset="/data/individual/sputum_collected" required="true()" type="string" 
					relevant="(../cough_gt_2_wks='y' or ../blood_cough_past_year='y' or ../fever_gt_3_wks = 'y' or ../weight_loss_past_year = 'y' 
							or ../tb_in_household = 'y' or ../failure_to_thrive = 'y') and ../current_tb_tx_status = 'n'" />
			<bind nodeset="/data/individual/follow_up" type="string" constraint="count-selected(.) &lt;= 3" jr:constraintMsg="Only a maximum of three allowed."/>
			<bind nodeset="/data/individual/follow_up_date" constraint=". &gt; today()" jr:constraintMsg="Follow-Up date must be in the future!" 
					relevant="../follow_up != ''" type="date"/>
			<!-- Group Bindings -->
			<bind nodeset="/data/household/conduct_survey_group" relevant="../allowed_in='accepted'" type="string"/>
			<bind nodeset="/data/household/economic_questions_group" type="string"/>
			<bind nodeset="/data/individual/children_below18_group" relevant="../approximate_age &lt; 216" type="string"/>
			<bind nodeset="/data/individual/children_below5_group" relevant="../approximate_age &lt; 60" type="string"/>
			<bind nodeset="/data/individual/immunization_info" relevant="../child_immunized='y'" type="string"/>
			<bind nodeset="/data/individual/hiv_testing_group" 
					relevant="../approximate_age &gt;= 156 or (../child_needs_hiv_test='Needs Testing' and ../approximate_age &gt;= 18) or (../test_child_regardless='y' and ../approximate_age &gt;= 18)" type="string"/>
			<bind nodeset="/data/individual/hiv_results_group" relevant="../accepted_hiv_testing='y'" type="string"/>
			<bind nodeset="/data/individual/tb_screening_group" type="string"/>
			<bind nodeset="/data/individual/tb_info_group" relevant="../current_tb_tx_status='n'" type="string"/>
		</model>
	</h:head>
	<h:body>
		<group>
			<label>Survey Information</label>
			<input ref="/data/survey/survey_location">
				<label>Survey Location</label>
				<hint>Ensure you have a clear view of the sky.</hint>
			</input>
			<input ref="/data/survey/survey_id">
				<label>Survey ID</label>
			</input>
			<input ref="/data/survey/provider_id">
				<label>Provider ID</label>
			</input>
			<select1 ref="/data/survey/team_id">
				<label>Team Number</label>
				<item>
					<label>Team 1</label>
					<value>team1</value>
				</item>
				<item>
					<label>Team 2</label>
					<value>team2</value>
				</item>
				<item>
					<label>Team 3</label>
					<value>team3</value>
				</item>
				<item>
					<label>Team 4</label>
					<value>team4</value>
				</item>
				<item>
					<label>Team 5</label>
					<value>team5</value>
				</item>
				<item>
					<label>Team 6</label>
					<value>team6</value>
				</item>
				<item>
					<label>Team 7</label>
					<value>team7</value>
				</item>
			</select1>
		</group>
		<group>
			<label>Household Information</label>
			<input ref="/data/household/household_id">
				<label>Household ID</label>
				<hint>Remember, a household is: 
 1. Father, wife/wives and children 
 2. Son, wife/wives and children in father's homestead
 3. Child headed home
 4. Mature man/woman living alone
 5. Daughter/son with child/children living in parent house
			</hint>
			</input>
			<input ref="/data/household/village">
				<label>Household Village</label>
			</input>
			<input ref="/data/household/household_adults">
				<label>Adults in Household</label>
				<hint>Must be more than 13 years old. If none, enter 0.</hint>
			</input>
			<input ref="/data/household/children_under13">
				<label>Children in Household</label>
				<hint>Must be less than 13 years old. If none, enter 0.</hint>
			</input>
			<input ref="/data/household/total_household">
				<label>Total in Household</label>
				<hint>Confirm this total number of adults and children with Mobilizer.</hint>
			</input>
			<select1 ref="/data/household/allowed_in">
				<label>Survey Status</label>
				<hint>Were you accepted in?</hint>
				<item>
					<label>Accepted</label>
					<value>accepted</value>
				</item>
				<item>
					<label>Declined</label>
					<value>declined</value>
				</item>
				<item>
					<label>Not Home</label>
					<value>nothome</value>
				</item>
				<item>
					<label>Requested Later Date</label>
					<value>laterdate</value>
				</item>
			</select1>
			<input ref="/data/household/return_date">
				<label>Schedule Return Date</label>
			</input>
		</group>
		<group ref="/data/household/conduct_survey_group">
			<label>Household Survey</label>
			<group ref="/data/household/economic_questions_group">
				<label>Economic Data</label>
				<input ref="/data/economics/land_owned">
					<label>Total Land Owned</label>
					<hint>Please enter in acres. If none, enter 0.</hint>
				</input>
				<input ref="/data/economics/cows_owned">
					<label>Number of Cows Owned</label>
					<hint>If none, enter 0.</hint>
				</input>
				<input ref="/data/economics/goats_owned">
					<label>Number of Goats Owned</label>
					<hint>If none, enter 0.</hint>
				</input>
				<input ref="/data/economics/sheep_owned">
					<label>Number of Sheep Owned</label>
					<hint>If none, enter 0.</hint>
				</input>
				<input ref="/data/economics/children_in_household">
					<label>Children Between 3-20 Years</label>
					<hint>Include all children in household.</hint>
				</input>
				<input ref="/data/economics/children_in_school">
					<label>Children Attending School</label>
					<hint>Include primary school through college.</hint>
				</input>
			</group>
			<group ref="/data/household/bednet_questions_group">
				<label>Bed Net Data</label>
				<input ref="/data/household/number_of_bednets">
					<label>Number of Bed Nets</label>
					<hint>If none, enter 0.</hint>
				</input>
				<select1 ref="/data/household/bednets_given">
					<label>Number of Nets Provided</label>
					<item>
						<label>Zero</label>
						<value>0</value>
					</item>
					<item>
						<label>One</label>
						<value>1</value>
					</item>
				</select1>
				<input ref="/data/household/bednet_voucher">
					<label>Voucher ID of Provided Net</label>
				</input>
			</group>
			<group>
				<label>Individual Section</label>
				<repeat nodeset="/data/individual">
					<input ref="/data/individual/household">
