<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">
	<h:head>
		<h:title>HCT Individual Survey</h:title>
		<model>
			<instance>
				<form name="HCT Individual Survey" version="1.0.0">
					<meta>
						<start_time/>
						<end_time/>
						<device_id/>
						<subscriber_id/>
					</meta>
					<individual>
						<household/>
						<hct_id_barcode/>
						<hct_id_manual/>
						<first_name/>
						<middle_name/>
						<family_name/>
						<gender/>
						<age_type/>
						<age_estimated/>
						<birth_date/>
						<age_in_years/>
						<age_in_months/>
						<approximate_age/>
						<phone_number/>
						<reenter_phone_number/>
						<demographics/>
						<marital_status/>
						<house_status/>
						<relation_to_househead/>
						<!-- Indiv Economics -->
						<hours_in_farm/>
						<hours_in_housework/>
						<clinic_visits/>
						<cost_of_medication/>
						<!-- End of Indiv Economics -->
						<pregnancy_status/>
						<attending_anc/>
						<children_below18_group/>
						<children_below5_group/>
						<mother_died/>
						<mother_hiv_positive/>
						<father_died/>
						<father_hiv_positive/>
						<child_needs_hiv_test/>
						<test_child_regardless/>
						<immunization_info/>
						<child_immunized/>
						<immunization_info_source/>
						<bcg_immunization/>
						<measles_immunization/>
						<previous_hiv_test/>
						<previous_hiv_test_year/>
						<previous_hiv_test_results/>
						<hiv_tx_program_status/>
						<hiv_tx_program_name/>
						<ampath_id/>
						<hiv_testing_group/>
						<pretest_counseling/>
						<accepted_hiv_testing/>
						<reasons_for_decline/>
						<hiv_results_group/>
						<test1_result/>
						<test2_name/>
						<test2_result/>
						<test3_name/>
						<test3_result/>
						<test_outcome/>
						<couple_tested_together/>
						<discordant_couple/>
						<partner_hct_id/>
						<tb_screening_group/>
						<current_tb_tx_status/>
						<tb_info_group/>
						<past_tb_tx_status/>
						<past_tb_tx_year/>
						<completed_8_mths_tb_tx/>
						<cough_gt_2_wks/>
						<blood_cough_past_year/>
						<fever_gt_3_wks/>
						<weight_loss_past_year/>
						<tb_in_household/>
						<failure_to_thrive/>
						<sputum_collected/>
						<follow_up_one/>
						<follow_up_two/>
						<follow_up_three/>
						<follow_up_date/>
						<preferred_clinic />
						<really_sick />
					</individual>
				</form>
			</instance>
			<!-- Meta Information Bindings -->
			<bind nodeset="/form/meta/start_time" jr:preload="timestamp" jr:preloadParams="start" type="time"/>
			<bind nodeset="/form/meta/end_time" jr:preload="timestamp" jr:preloadParams="end" type="time"/>
			<bind nodeset="/form/meta/device_id" jr:preload="property" jr:preloadParams="deviceid" type="string"/>
			<bind nodeset="/form/meta/subscriber_id" jr:preload="property" jr:preloadParams="subscriberid" type="string"/>
			
			<bind nodeset="/form/individual/household" constraint="review('Household ID', .)" required="true()" type="lookup" />
   			<bind nodeset="/form/individual/hct_id_barcode" constraint="regex(., '^\w+[-]?\d{1}') and checkdigit(.) and unique('individual', .)" 						jr:constraintMsg="Invalid HCT ID. (Note: The ID may already have been assigned.) Please check and re-enter." type="barcode"/>
			<bind nodeset="/form/individual/hct_id_manual" relevant="../hct_id_barcode = '' " required="true()" 
					constraint="regex(., '^\w+[-]?\d{1}') and checkdigit(.) and unique('individual', .) and review('HCT ID', .)" 
					jr:constraintMsg="Invalid HCT ID. (Note: The ID may already have been assigned.) Please check and re-enter." type="string"/>
			<bind nodeset="/form/individual/first_name" constraint="review('Given Name', .)" required="true()" type="string"/>
			<bind nodeset="/form/individual/middle_name" constraint="review('Middle Name', .)" type="string"/>
			<bind nodeset="/form/individual/family_name" constraint="review('Family Name', .)" required="true()" type="string"/>
			<bind nodeset="/form/individual/gender" constraint="review('Gender', .)" required="true()" type="string"/>
			<bind nodeset="/form/individual/age_type" required="true()" type="string"/>
			<bind nodeset="/form/individual/age_estimated" required="true()" type="string"/>
			<bind nodeset="/form/individual/birth_date" relevant="../age_type='birth_date'" constraint=". &lt;= today()" 
					jr:constraintMsg="Birth dates must be before today." type="date"/>
			<bind nodeset="/form/individual/age_in_years" constraint=". &gt;= 0 and . &lt;= 110" 
					relevant="../age_type='age_in_years_months' or ../age_type='unknown'" required="true()" type="integer"/>
			<bind nodeset="/form/individual/age_in_months" constraint=". &gt;= 0 and . &lt;= 11" required="true()" type="integer"
					relevant="(../age_type='age_in_years_months' or ../age_type='unknown') and ../age_in_years &lt; 5" />
			<bind nodeset="/form/individual/approximate_age" readonly="true()" type="integer" 
					calculate="if(../age_type='birth_date', ((now()-../birth_date) div 30), (if(../age_in_months &gt;0, 
					((../age_in_years * 12)+../age_in_months), (../age_in_years * 12))))"/>
			<bind nodeset="/form/individual/phone_number" required="true()" 
					constraint="(. = '0' or regex(., '\d{8,12}')) and review('Phone Number', .) and review('Year of Birth', ../approximate_age)" 						jr:constraintMsg="Invalid Phone Number. Please check and re-enter." type="string"/>
			<bind nodeset="/form/individual/reenter_phone_number" type="string" relevant="../phone_number != '0'" required="true()" 
					constraint=". = ../phone_number" jr:constraintMsg="Phone numbers do not match. Go back and confirm both."/>
			<!-- can't have review type -->
			<bind nodeset="/form/individual/demographics" type="review"/>
			<bind nodeset="/form/individual/marital_status" required="true()" relevant="../approximate_age &gt;= 96" type="string"/>
			<bind nodeset="/form/individual/house_status" required="true()" type="string"/>
			<bind nodeset="/form/individual/relation_to_househead" required="true()" type="string"
					constraint="not(selected(., 'self')) or (selected(.,'self') and selected(../house_status,'household_resident'))"
					jr:constraintMsg="Household already has a head or this is a non-resident individual. 'Can't be household head'." />
			<!-- Individual Economic Questions -->
			<bind nodeset="/form/individual/hours_in_farm" required="true()" relevant="../approximate_age &gt;= 156" type="integer"/>
			<bind nodeset="/form/individual/hours_in_housework" required="true()" relevant="../approximate_age &gt;= 156" type="integer"/>
			<bind nodeset="/form/individual/clinic_visits" required="true()" type="integer"/>
			<bind nodeset="/form/individual/cost_of_medication" required="true()" type="integer"/>
			<!-- End of Individual Economic Questions -->
			<bind nodeset="/form/individual/pregnancy_status" required="true()" relevant="../gender='f' and ../approximate_age &gt;= 96" type="string"/>
			<bind nodeset="/form/individual/attending_anc" required="true()" relevant="../pregnancy_status='y'" type="string"/>
			<bind nodeset="/form/individual/mother_died" required="true()" type="string"/>
			<bind nodeset="/form/individual/mother_hiv_positive" required="true()" type="string"/>
			<bind nodeset="/form/individual/father_died" required="true()" type="string"/>
			<bind nodeset="/form/individual/father_hiv_positive" required="true()" type="string"/>
			<bind nodeset="/form/individual/child_needs_hiv_test" readonly="true()" 
					calculate="if(../approximate_age &lt; 156 and (../mother_died='y' or ../mother_hiv_positive='y'), 
					'Needs Testing', 'Does Not Need Testing')" 
					relevant="../approximate_age &lt; 156 and (../mother_died='y' or ../mother_hiv_positive='y')" type="string"/>
			<bind nodeset="/form/individual/test_child_regardless" 
					relevant="../approximate_age &lt; 156 and (../mother_died='n' and ../mother_hiv_positive='n')" type="string"/>
			<bind nodeset="/form/individual/child_immunized" type="string"/>
			<bind nodeset="/form/individual/immunization_info_source" type="string"/>
			<bind nodeset="/form/individual/bcg_immunization" required="true()" type="string"/>
			<bind nodeset="/form/individual/measles_immunization" required="true()" type="string"/>
			<bind nodeset="/form/individual/previous_hiv_test" required="true()" type="string"/>
			<bind nodeset="/form/individual/previous_hiv_test_year" required="true()" relevant="../previous_hiv_test='y'" type="string"/>
			<bind nodeset="/form/individual/previous_hiv_test_results" required="true()" relevant="../previous_hiv_test='y'" type="string"/>
			<bind nodeset="/form/individual/hiv_tx_program_status" relevant="../previous_hiv_test_results='positive'" type="string"/>
			<bind nodeset="/form/individual/hiv_tx_program_name" relevant="../hiv_tx_program_status='y'" type="string"/>
			<bind nodeset="/form/individual/ampath_id" type="string" 
					relevant="../hiv_tx_program_name='Ampath' or ../hiv_tx_program_name='AMPATH' or ../hiv_tx_program_name='ampath'"/>
			<bind nodeset="/form/individual/pretest_counseling" required="true()" type="string"/>
			<bind nodeset="/form/individual/accepted_hiv_testing" required="true()" type="string" 
					constraint=". = 'n' or (. ='y' and ../pretest_counseling='y')" jr:constraintMsg="You can not test without counseling"/>
			<bind nodeset="/form/individual/reasons_for_decline" required="true()" type="string" 
					relevant="../accepted_hiv_testing='n' and ../approximate_age &lt; 156" constraint="count-selected(.) &lt;= 3" 						jr:constraintMsg="Only a maximum of three allowed."/>
			<bind nodeset="/form/individual/test1_result" required="true()" type="string"/>
			<bind nodeset="/form/individual/test2_name" required="true()" type="string"/>
			<bind nodeset="/form/individual/test2_result" required="true()" type="string"/>
			<bind nodeset="/form/individual/test3_name" required="true()" relevant="../test2_result != ../test1_result" constraint=". != ../test2_name" 						jr:constraintMsg="Test 3 must be different from Test 2!" type="string"/>
			<bind nodeset="/form/individual/test3_result" required="true()" relevant="../test2_result != ../test1_result" type="string"/>
			<bind nodeset="/form/individual/test_outcome" readonly="true()" relevant="../test2_result != '' or ../test3_result != ''" type="string" 					calculate="if (../test3_result = 'reactive' or ((../test1_result = ../test2_result) and ../test2_result = 'reactive'),
					'Please refer patient to AMPATH', if (../test3_result = 'nonreactive' or ((../test1_result = ../test2_result) 
					and ../test2_result = 'nonreactive'),'Patient is Negative', 'Refer patient to a clinic for further testing.'))"/>
			<bind nodeset="/form/individual/couple_tested_together" required="true()" relevant="../marital_status='married'" type="string"/>
			<bind nodeset="/form/individual/discordant_couple" required="true()" relevant="../marital_status='married'" type="string"/>
			<!-- check partner hct id -->
			<bind nodeset="/form/individual/partner_hct_id" constraint="regex(., '^\w+[-]?\d{1}') and checkdigit(.)" required="true()" 
					relevant="../discordant_couple='y'" type="string"/>
			<bind nodeset="/form/individual/current_tb_tx_status" required="true()" type="string"/>
			<bind nodeset="/form/individual/past_tb_tx_status" required="true()" type="string"/>
			<bind nodeset="/form/individual/past_tb_tx_year" required="true()" relevant="../past_tb_tx_status='y'" type="string"/>
			<bind nodeset="/form/individual/completed_8_mths_tb_tx" required="true()" relevant="../past_tb_tx_status='y'" type="string"/>
			<bind nodeset="/form/individual/cough_gt_2_wks" required="true()" type="string"/>
			<bind nodeset="/form/individual/blood_cough_past_year" required="true()" type="string"/>
			<bind nodeset="/form/individual/fever_gt_3_wks" required="true()" type="string"/>
			<bind nodeset="/form/individual/weight_loss_past_year" required="true()" type="string"/>
			<bind nodeset="/form/individual/tb_in_household" required="true()" type="string"/>
			<bind nodeset="/form/individual/failure_to_thrive" relevant="../approximate_age &lt; 156" type="string"/>
			<bind nodeset="/form/individual/sputum_collected" required="true()" type="string"
					relevant="(../cough_gt_2_wks='y' or ../blood_cough_past_year='y' or ../fever_gt_3_wks = 'y' or ../weight_loss_past_year = 'y' 
							or ../tb_in_household = 'y' or ../failure_to_thrive = 'y') and ../current_tb_tx_status = 'n'" />
			<bind nodeset="/form/individual/follow_up_one" type="string" required="true()" 
					constraint=". != ../follow_up_two and . != ../follow_up_three" jr:constraintMsg="This Follow up has already been selected." />
			<bind nodeset="/form/individual/follow_up_two" type="string" required="true()" relevant="not(selected(../follow_up_one, 'none'))" 
					constraint=". != ../follow_up_three and . != ../follow_up_one" jr:constraintMsg="This Follow up has already been selected." />
			<bind nodeset="/form/individual/follow_up_three" type="string" required="true()" relevant="not(selected(../follow_up_two, 'none'))" 
					constraint=". != ../follow_up_two and . != ../follow_up_one" jr:constraintMsg="This Follow up has already been selected."/>
			<bind nodeset="/form/individual/follow_up_date" constraint=". &gt; today()" jr:constraintMsg="Follow-Up date must be in the future!" 
					relevant="not(selected(../follow_up_one, 'none'))" type="date"/>
			<bind nodeset="/form/individual/preferred_clinic" relevant="not(selected(../follow_up_one, 'none'))" type="string"/>
			<bind nodeset="/form/individual/really_sick" type="string"/>
			
			<!-- Group Bindings -->
			<bind nodeset="/form/individual/children_below18_group" relevant="../approximate_age &lt; 216" type="string"/>
			<bind nodeset="/form/individual/children_below5_group" relevant="../approximate_age &lt; 60" type="string"/>
			<bind nodeset="/form/individual/immunization_info" relevant="../child_immunized='y'" type="string"/>
			<bind nodeset="/form/individual/hiv_testing_group" 
					relevant="../approximate_age &gt;= 156 or (../child_needs_hiv_test='Needs Testing' 
					and ../approximate_age &gt;= 18) or (../test_child_regardless='y' and ../approximate_age &gt;= 18)" type="string"/>
			<bind nodeset="/form/individual/hiv_results_group" relevant="../accepted_hiv_testing='y'" type="string"/>
			<bind nodeset="/form/individual/tb_screening_group" type="string"/>
			<bind nodeset="/form/individual/tb_info_group" relevant="../current_tb_tx_status='n'" type="string"/>
		</model>
	</h:head>
	<h:body>
		<input ref="/form/individual/household">
			<label>Household Counseling and Testing for the Household ID
			</label>
			<hint>Please confirm this ID is correct.</hint>
		</input>
		<group>
			<label>Individual Section</label>
			<repeat nodeset="/form/individual">
				<input ref="/form/individual/first_name">
					<label>First Name</label>
				</input>
				<input ref="/form/individual/middle_name">
					<label>Middle Name</label>
				</input>
				<input ref="/form/individual/family_name">
					<label>Family Name</label>
				</input>
				<select1 ref="/form/individual/gender">
					<label>Gender</label>
					<item>
						<label>Male</label>
						<value>m</value>
					</item>
					<item>
						<label>Female</label>
						<value>f</value>
					</item>
				</select1>
				<!--Individual Age-->
				<select1 ref="/form/individual/age_type">
					<label>Age Entry Mode</label>
					<item>
						<label>Enter Full Birthdate</label>
						<value>birth_date</value>
					</item>
					<item>
						<label>Enter Age in Years or Months</label>
						<value>age_in_years_months</value>
					</item>
				</select1> 
				<select1 ref="/form/individual/age_estimated">
					<label>Age Estimated</label>
					<item>
						<label>Yes</label>
						<value>y</value>
					</item>
					<item>
						<label>No</label>
						<value>n</value>
					</item>
				</select1>
<!-- If enter birthdate -->
				<input ref="/form/individual/birth_date">
					<label>Select Birthdate</label>
				</input>
<!-- If age in years or Unknown -->
				<input ref="/form/individual/age_in_years">
					<label>Number of Years</label>
				</input>
				<input ref="/form/individual/age_in_months">
					<label>Number of Months</label>
				</input>
				<input ref="/form/individual/phone_number">
					<label>Enter Phone Number</label>
					<hint>If none, enter 0.</hint>
				</input>
				<input ref="/form/individual/reenter_phone_number">
					<label>Re-Enter Phone Number</label>
					<hint>If none, enter 0.</hint>
				</input>
				<input ref="/form/individual/demographics">
					<label>Individual Demographics</label>
					<hint>Please write this demographics on the card and issue it out to the client.</hint>
				</input>
				<input ref="/form/individual/hct_id_barcode">
					<label>Scan HCT AMRS ID</label>
					<hint>Ensure you have enough light.</hint>
				</input>
				<input ref="/form/individual/hct_id_manual">
					<label>Enter HCT AMRS ID</label>
				</input>
				<select1 ref="/form/individual/marital_status">
					<label>Marital Status</label>
					<item>
						<label>Single</label>
						<value>single</value>
					</item>
					<item>
						<label>Married</label>
						<value>married</value>
					</item>
					<item>
						<label>Divorced</label>
						<value>divorced</value>
					</item>
					<item>
						<label>Separated</label>
						<value>separated</value>
					</item>
					<item>
						<label>Widowed</label>
						<value>widowed</value>
					</item>
					<item>
						<label>Cohabiting</label>
						<value>cohabiting</value>
					</item>
				</select1>
				<select1 ref="/form/individual/house_status">
					<label>Household Status</label>
					<hint>A household resident is a person who is part of a household (including servants/employees).</hint>
					<item>
						<label>Household Resident</label>
						<value>household_resident</value>
					</item>
					<item>
						<label>Visitor from Catchment</label>
						<value>visitor_catchment</value>
					</item>
					<item>
						<label>Visitor from Elsewhere</label>
						<value>visitor_elsewhere</value>
					</item>
				</select1>
				<select1 ref="/form/individual/relation_to_househead">
					<label>Relation to Head of Household</label>
					<item>
						<label>Self</label>
						<value>self</value>
					</item>
					<item>
						<label>Spouse</label>
						<value>spouse</value>
					</item>
					<item>
						<label>Child</label>
						<value>child</value>
					</item>
					<item>
						<label>Parent</label>
						<value>parent</value>
					</item>
					<item>
						<label>Sibling</label>
						<value>sibling</value>
					</item>
					<item>
						<label>Grand Parent</label>
						<value>grand_parent</value>
					</item>
					<item>
						<label>Grand Child</label>
						<value>grand_child</value>
					</item>
					<item>
						<label>Father/Mother in Law</label>
						<value>parent_in_law</value>
					</item>
					<item>
						<label>Brother/Sister in Law</label>
						<value>sibiling_in_law</value>
					</item>
					<item>
						<label>Step Parent</label>
						<value>step_parent</value>
					</item>
					<item>
						<label>Step Child</label>
						<value>step_child</value>
					</item>
					<item>
						<label>Aunt/Uncle</label>
						<value>aunt_uncle</value>
					</item>
					<item>
						<label>Niece/Nephew</label>
						<value>niece_nephew</value>
					</item>
					<item>
						<label>Cousin</label>
						<value>cousin</value>
					</item>
					<item>
						<label>Foster Child</label>
						<value>foster_child</value>
					</item>
					<item>
						<label>Friend</label>
						<value>friend</value>
					</item>
					<item>
						<label>House Help</label>
						<value>house_help</value>
					</item>
					<item>
						<label>Employee</label>
						<value>employee</value>
					</item>
					<item>
						<label>Tenant/Renter</label>
						<value>tenant_renter</value>
					</item>
					<item>
						<label>Other</label>
						<value>other</value>
					</item>
					<item>
						<label>Unknown</label>
						<value>unknown</value>
					</item>
				</select1>
<!-- Indiv Economic Qstns -->
				<input ref="/form/individual/hours_in_farm">
					<label>Hours Worked Past Week on Farm</label>
					<hint>Include for wage and business.</hint>
				</input>
				<input ref="/form/individual/hours_in_housework">
					<label>Hours Worked Past Week in Housework</label>
					<hint>For example, cooking and cleaning.</hint>
				</input>
				<input ref="/form/individual/clinic_visits">
					<label>Times Visited Clinic Due to Sickness in Last Month</label>
					<hint>If none, enter 0.</hint>
				</input>
				<input ref="/form/individual/cost_of_medication">
					<label>Amount of Money Spent on Medication in Last Month</label>
					<hint>Value in Kshs</hint>
				</input>
<!-- End of Indiv Qsts -->
				<select1 ref="/form/individual/pregnancy_status">
					<label>Pregnant</label>
					<item>
						<label>Yes</label>
						<value>y</value>
					</item>
					<item>
						<label>No</label>
						<value>n</value>
					</item>
				</select1>
				<select1 ref="/form/individual/attending_anc">
					<label>Attending Antenatal Clinic</label>
					<item>
						<label>Yes</label>
						<value>y</value>
					</item>
					<item>
						<label>No</label>
						<value>n</value>
					</item>
				</select1>
<!-- Start -For Children < 18 years -->
				<group ref="/form/individual/children_below18_group">
					<label>Child Below 18</label>
					<select1 ref="/form/individual/mother_died">
						<label>Mother Died</label>
						<item>
							<label>Yes</label>
							<value>y</value>
						</item>
						<item>
							<label>No</label>
							<value>n</value>
						</item>
						<item>
							<label>Unknown</label>
							<value>unknown</value>
						</item>
					</select1>
					<select1 ref="/form/individual/mother_hiv_positive">
						<label>Mother HIV Positive</label>
						<item>
							<label>Yes</label>
							<value>y</value>
						</item>
						<item>
							<label>No</label>
							<value>n</value>
						</item>
						<item>
							<label>Unknown</label>
							<value>unknown</value>
